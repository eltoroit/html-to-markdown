	"version": "0.2.0",
	"configurations": [
		{
			"name": "ET: Debug Agentforce PTO",
			"type": "node",
			"request": "launch",
			"cwd": "${workspaceFolder}",
			"runtimeExecutable": "npm",
			"runtimeArgs": ["run", "serve"],
			"env": {
				"DEBUG_DENO": "--inspect-brk"
			},
			"console": "integratedTerminal",
			"internalConsoleOptions": "neverOpen",
			"attachSimplePort": 9229
		},
		{
			"name": "Attach",
			"type": "node",
			"request": "attach",
			"skipFiles": ["<node_internals>/**"],
			"internalConsoleOptions": "neverOpen",
			"port": 9229
		}
